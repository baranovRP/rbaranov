<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ru.job4j.model.car.parts.CarModel" table="CAR_MODEL"
           dynamic-update="true">
        <id name="id" type="java.lang.Long">
            <column name="ID"/>
            <generator class="identity"/>
        </id>
        <property name="name" column="NAME"/>
        <many-to-one name="manufacture" column="MANUFACTURE_ID" lazy="false"
                     class="ru.job4j.model.car.parts.Manufacture"
                     not-null="true"/>
    </class>
</hibernate-mapping>